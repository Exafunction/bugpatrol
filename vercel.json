{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/index.js"
    }
  ],
  "crons": [
    {
      "path": "/api/update-bug-status",
      "schedule": "0 9 * * 1-5"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
